<style>
  body { background-image: url(<%= asset_path 'image-asset.jpg' %>) }
</style>
<h1>Log in And Start Adventuring MiniRpg User</h1>

<%= form_tag login_index_path, class: "auth-form" do %>
   <ul>
    <li>
      <%= label_tag :username %>
      <%= text_field_tag :username %>
    </li>
    <li>
      <%= label_tag :password %>
      <%= password_field_tag :password %>
    </li>
  </ul>
  <%= submit_tag "Log in" %>
<% end %>




